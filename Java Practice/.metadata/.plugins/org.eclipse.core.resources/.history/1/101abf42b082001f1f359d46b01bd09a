package dryRunPractice;

import java.util.Scanner;

public class SumOfDiv {

	public static void main(String[] args) {
		Scanner s= new Scanner(System.in);
		System.out.println("Enter the number :");
		int a=12;
		int i=1;
		int sum=0;
		
		while(i<=a/2) {
		
			if(a%i==0) {
				sum=sum+i;
					
			}
			i++;
			
			
		}
		System.out.println(sum);
		
		

	}

}
