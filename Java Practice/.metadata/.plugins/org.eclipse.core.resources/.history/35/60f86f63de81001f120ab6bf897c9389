package loopExersise;

public class BasicLoop {
	
	
	public void printEven() {
	for(int i=1; i<=100; i++) {
		
		//printing even nmber
		if (i%2==0) {
			
			System.out.print(i+" ");
		}
		
	
	}
	}
	//wap to print the even number b/w 1 to 100 in an increasing order.
	public static void main(String[] args) {
		BasicLoop obj=new BasicLoop();
		obj.printEven();
		
	}

}
