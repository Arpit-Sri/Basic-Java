import java.util.Scanner;

public class Demo1{

	public static void main(String[] args){
	
	Scanner sc=new Scanner(System.in);
	
	int x=0;

	
	do{
		try{
	System.out.print("Enter the number :");
	x+=sc.nextInt();
	System.out.println(x);
		}catch(Exception e){
			System.out.println("something went wrong ");
			sc.next(); 
		}
	}while(false);
	

	
		
	
   }

}
